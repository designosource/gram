{% extends "_layout" %}

{% block content %}

	<div class="section">
		<div class="container">
			<div class="grid">
				<div class="row">
					{% for item in craft.entries.section('partners').order('title ASC') %}
						<div class="col-4">
							<p style="cursor: default" class="link" data-toggle="tooltip" data-placement="bottom" title="{{ item.address }}">
								{{ item.title }}
							</p>
						</div>
					{% endfor %}
				</div>
			</div>
		</div>
	</div>
	
{% endblock %}